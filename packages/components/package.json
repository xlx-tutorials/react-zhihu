{
  "name": "@react-zhihu/components",
  "version": "0.2.5",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently 'yarn:build:* --watch'",
    "build": "del-cli dist && concurrently yarn:build:*",
    "build:types": "tsc -p ./tsconfig.types.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "test": "concurrently yarn:test:*",
    "test:unit": "jest",
    "test:typechecks": "tsc -p ./tsconfig.checks.json"
  },
  "devDependencies": {
    "@jest/types": "^27.1.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.20",
    "concurrently": "^6.2.1",
    "del-cli": "^4.0.1",
    "jest": "^27.1.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
