{
  "name": "@react-zhihu/components",
  "version": "0.2.9",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently 'yarn:build:* --watch'",
    "build": "del-cli dist && concurrently yarn:build:*",
    "build:types": "tsc -p ./tsconfig.types.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "test": "concurrently yarn:test:*",
    "test:unit": "jest",
    "test:typechecks": "tsc -p ./tsconfig.checks.json",
    "test:lint": "eslint '**/*.{ts,tsx}' --color --fix",
    "test:format": "prettier 'src/**/*.{ts,tsx,js,jsx,mdx}' --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o dist-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@jest/types": "^27.1.1",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^6.2.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {}
}
