{
  "name": "@react-zhihu/components",
  "version": "0.2.11",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "ðŸ’¡ ------ All": "",
    "start": "concurrently yarn:start-components yarn:start-storybook",
    "build": "yarn build-components && yarn build-storybook",
    "test": "concurrently yarn:test-components",
    "ðŸ“¦ ------ Components": "",
    "start-components": "concurrently 'yarn:build-components:* --watch'",
    "build-components": "del-cli dist && concurrently yarn:build-components:*",
    "build-components:types": "tsc -p ./tsconfig.types.json",
    "build-components:esm": "tsc -p ./tsconfig.esm.json",
    "build-components:cjs": "tsc -p ./tsconfig.cjs.json",
    "test-components": "concurrently yarn:test-components:*",
    "test-components:unit": "jest",
    "test-components:typechecks": "tsc -p ./tsconfig.checks.json",
    "test-components:lint": "eslint '**/*.{ts,tsx}' --color --fix",
    "test-components:format": "prettier 'src/**/*.{ts,tsx,js,jsx,mdx}' --write",
    "ðŸ“š ------ Storybook": "",
    "start-storybook": "start-storybook -p 6006 --quiet",
    "build-storybook": "build-storybook -o dist-storybook --quiet"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@jest/types": "^27.1.1",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "concurrently": "^6.2.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {}
}
